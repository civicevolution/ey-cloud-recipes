check process <%= @worker_name %>
	with pidfile /var/run/<%= @worker_name %>.pid
  start program = "/bin/bash /data/<%=@app_name %>/current/script/monit.notify.sh start"
  stop program = "/bin/bash /data/<%=@app_name %>/current/script/monit.notify.sh stop"
  if totalmem is greater than 60 MB for 2 cycles then restart      # eating up memory?
